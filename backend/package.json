{
  "name": "backend",
  "version": "0.1.0",
  "description": "Company health job scraping and analysis backend",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir=dist --target=bun",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "prisma": "prisma",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "elt:ashby": "bun run scripts/run-ashby-elt.ts",
    "elt:news": "bun run scripts/run-news-elt.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/pg": "^8.16.0",
    "typescript": "^5.6.0"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@tavily/core": "^0.7.1",
    "@vercel/blob": "^2.2.0",
    "pg": "^8.18.0",
    "prisma": "^7.3.0",
    "tslog": "^4.10.2"
  }
}
